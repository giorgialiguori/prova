<?xml version="1.0" encoding="UTF-8" ?>
<mvc:View
    controllerName="com.giorgia.provaUI5.controller.generic.Slide"
    xmlns:core="sap.ui.core"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
    class="viewPadding">

    <Page showNavButton="true"
    navButtonPress="handleNavButtonPress"
    titleLevel="H1">


        <Panel expandable="false"
            expanded="true"
            width ="5rem"

            class="sapUiResponsiveMargin">
            <content>

              <ToolbarSpacer />
              <Button tooltip="{app>/slideMenu/configuration}" icon="sap-icon://settings" press="handleSetButtonPress"/>
            </content>
        </Panel>
        <Panel expandable="true"
            expanded="false"
            width="auto"
            class="sapUiResponsiveMargin">

            <content>

            </content>
        </Panel>
        <Panel expandable="true"
            expanded="false"

            width="auto"
            class="sapUiResponsiveMargin">
            <content>

            </content>
        </Panel>

        
    </Page>

</mvc:View>
